<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="647.0" xmlns="http://javafx.com/javafx/18" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controllers.LoginController">
   <children>
      <Label alignment="CENTER" layoutX="229.0" layoutY="45.0" prefHeight="40.0" prefWidth="190.0" text="Logowanie">
         <font>
            <Font size="37.0" />
         </font>
      </Label>
      <Label alignment="BOTTOM_LEFT" layoutX="55.0" layoutY="111.0" prefHeight="27.0" prefWidth="538.0" text="Email">
         <font>
            <Font size="18.0" />
         </font></Label>
      <TextField id="email_field" fx:id="emailField" layoutX="56.0" layoutY="138.0" prefHeight="26.0" prefWidth="538.0" promptText="Email">
         <font>
            <Font size="16.0" />
         </font></TextField>
      <Label layoutX="55.0" layoutY="187.0" prefHeight="27.0" prefWidth="538.0" text="Hasło">
         <font>
            <Font size="18.0" />
         </font></Label>
      <PasswordField fx:id="passwordField" layoutX="55.0" layoutY="214.0" prefHeight="26.0" prefWidth="538.0" promptText="Hasło">
         <font>
            <Font size="16.0" />
         </font></PasswordField>
      <Button fx:id="loginButton" alignment="BOTTOM_CENTER" layoutX="270.0" layoutY="311.0" mnemonicParsing="false" onAction="#userLogin" prefHeight="30.0" prefWidth="108.0" text="Zaloguj" textAlignment="CENTER">
         <font>
            <Font size="16.0" />
         </font>
      </Button>
      <Label fx:id="wrongLogin" alignment="CENTER" layoutX="55.0" layoutY="270.0" prefHeight="30.0" prefWidth="538.0" />
      <Button fx:id="backButton" layoutX="14.0" layoutY="14.0" mnemonicParsing="false" onAction="#goBack" prefHeight="31.0" prefWidth="75.0" text="Powrót">
         <font>
            <Font size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
